<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="eu.wisebed.wiseui.shared.wiseml.SecretAuthenticationKey" table="SECRET_AUTHENTICATION_KEY">
        <id name="secretAuthenticationKeyID" column="secret_authentication_key_id">
        	<generator class="increment"/>
        </id>
        <property name="username" type="string"/>
        <property name="secretAuthenticationKey" type="string" length="512"/>
        <property name="urnPrefix" type="string"/>

    </class>
</hibernate-mapping>