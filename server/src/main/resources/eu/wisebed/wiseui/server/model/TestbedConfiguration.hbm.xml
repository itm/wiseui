<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="eu.wisebed.wiseui.shared.TestbedConfiguration"
		table="TESTBED">
		<id name="testbedID" column="testbed_id"></id>
		<property name="name" />
		<property name="rsEndpointUrl" />
		<property name="sessionmanagementEndpointUrl" />
		<property name="snaaEndpointUrl" />
		<property name="testbedUrl" />

		<list name="urnPrefixList" table="URNPREFIX">
			<key column="testbedID" not-null="true" />
			<list-index column="sort_order" base="0" />
			<element column="urnprefix" type="string" />
		</list>

	</class>
</hibernate-mapping>