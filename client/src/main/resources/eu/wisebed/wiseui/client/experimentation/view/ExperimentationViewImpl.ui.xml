<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
		.experimentMonitoringMainPanel {
			margin: 1px;
			padding: 1px;
			height:auto;
			width:auto;
		}
		
		.fullLayout {
        	width: 100%;
            height: 100%;
        }
	</ui:style>
	<g:ScrollPanel styleName="{style.fullLayout}">
		<g:FlowPanel styleName="{style.experimentMonitoringMainPanel}" ui:field="experimentContainer"/>
	</g:ScrollPanel>
</ui:UiBinder>