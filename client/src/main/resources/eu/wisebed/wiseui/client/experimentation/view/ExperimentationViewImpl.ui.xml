<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui" 
			 xmlns:my="urn:import:eu.wisebed.wiseui.widgets">


	<ui:style>
		.fullLayout {
			width: 100%;
			height: 100%;
		}
	</ui:style>

	<ui:with field='famfamfam'
		type="eu.wisebed.wiseui.widgets.famfamfam.FamFamFamResources" />

	<my:CaptionPanel caption="Experiments" ui:field="container"
		styleName="{style.fullLayout}">
		<g:VerticalPanel>
			<g:SimplePanel ui:field="experimentContainer" styleName="{style.fullLayout}">
				<g:VerticalPanel>
				</g:VerticalPanel>
			</g:SimplePanel>
		</g:VerticalPanel>
	</my:CaptionPanel>
</ui:UiBinder>