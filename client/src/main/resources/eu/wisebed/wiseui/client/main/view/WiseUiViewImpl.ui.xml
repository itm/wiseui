<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
		.border {
			border: 5px solid #92C1F0;
		}
	</ui:style>

	<g:DockLayoutPanel>
		<g:north size="24">
			<!-- <g:VerticalPanel> <g:HTMLPanel> <h1 style="padding:10px;">WiseUI</h1> 
				</g:HTMLPanel> -->
			<g:SimplePanel ui:field="navigationPanel" />
			<!-- </g:VerticalPanel> -->
		</g:north>
		<g:center>
			<g:SplitLayoutPanel addStyleNames="{style.border}">
				<g:center>
					<g:SimplePanel ui:field="contentPanel" />
				</g:center>
				<g:west size="250">
					<g:SimplePanel ui:field="testbedListPanel" />
				</g:west>
			</g:SplitLayoutPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>